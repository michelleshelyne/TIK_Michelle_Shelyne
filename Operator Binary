#include <iostream>
#include <math.h>
using namespace std;

int main (){
    //operator aritmatika
    int a = 10;
    int b = 5;
    
   int hasiltambah = a + b; //penjumlahan
   int hasilkurang = a - b; //pengurangan
   int hasilbagi = a / b; //pembagian
   int hasilkali = a * b; //perkalian
   int sisabagi = a % b;
   
   cout<<a + b<<endl;
   cout<<a - b<<endl;
   cout<<a / b<<endl;
   cout<<a * b<<endl;
   cout<<a % b<<endl;
    
    return 0;
}
